<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.myadminmain.workflow.model.dao.ActModelMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.myadminmain.workflow.model.entity.ActModel">
        <id column="id" property="id"/>
        <result column="model_id" property="modelId"/>
        <result column="model_key" property="modelKey"/>
        <result column="model_name" property="modelName"/>
        <result column="model_version" property="modelVersion"/>
        <result column="remark" property="remark"/>
        <result column="create_user" property="createUser"/>
        <result column="create_time" property="createTime"/>
        <result column="update_user" property="updateUser"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        A.id,
        A.model_id,
        A.model_key,
        A.model_name,
        A.model_version,
        A.remark,
        A.create_user,
        A.create_time,
        A.update_user,
        A.update_time
    </sql>

    <!-- 表名称 -->
    <sql id="tableName">
        sys_act_model A
    </sql>
</mapper>
